<!DOCTYPE html>
<html>
  <head>
    <title>StackOverflow</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div id="user_header">
        <%= link_to "Sign Up", new_user_path %>
      </div>

    </div>
      <%= flash[:alert] %>
      <%= flash[:notice] %>

    <%= yield %>
  </body>
</html>
